{ "type": "FeatureCollection",
  "features": [
  {% for co in qs %}
    { "type": "Feature",
      "geometry" : {{ co.geojson|safe }},
      "properties" : {
        "id" : {{ co.id }},
        "user" : "{{ co.user.username }}"
      }
    }
   {% if not forloop.last %},{% endif %} 
  {% endfor %} 
  ]
}
  

  